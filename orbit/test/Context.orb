module Orb::Types::Intrinsics {
    type Unit
    type Int
}

module Orb::Meta::Contexts {
    context Eq [A, B]
        where A = B

    context KindEq [A, B]
        where A ^ B

    context Like [A, B]
        where A : B

    context Has [A, F]
        where P ^ Kinds::Field
        where P in A
}