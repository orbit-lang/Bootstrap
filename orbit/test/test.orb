api Comparator {
    required type ComparableType
    required type ResultType
}

module IntComp : Comparator with Orb::Types::Intrinsics with Comparator {
    type ComparableType = Int
    type ResultType = Bool

    (IntComp) compare (a ComparableType, b ComparableType) (ResultType) {
        return ResultType()
    }
}