module Orb::Types::Intrinsics {
    type Unit
    type Int
}

module Orb::Core::Main {
    type Main
}

module Test with Orb::* {
    type StatusCode(code Int)

    (self StatusCode) intValue () (Int) = (self.code)

    (self Main) main _ _ {
        sc = StatusCode(_)
        iv = StatusCode::intValue
        r = invoke iv(sc)
    }
}
