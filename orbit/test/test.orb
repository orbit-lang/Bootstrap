@Main
module M with Orb::Core::* with Orb::Compiler::* {
    type constructor Box<T>(value T)
    trait A(value Int)
    type B(value Int) : A

    extension Box where Self[T] : A {
        (self Self) getValue() (AnyType) = self.value
    }

    (self Main) main () () {
        b = Box(B(2))
        print b.getValue()
    }
}