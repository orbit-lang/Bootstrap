module Orb::Core::Types {
    type Unit
}

module Orb::Core::Booleans {
    type Bool {
        constructor True
        constructor False
    }
}

module Orb::Core::Numbers {
    type Int
    type Real
}

module Orb::Core::Errors with Orb::Core::* {
    trait Error
}

module Main with Orb::* {
    attribute Same = (A, B) => A = B
    attribute Like = (A, B) => A : B

    fun (Unit) main () () {
        typeOf ${1 + 1}
    }
}